{
  "redirects": [
    {
      "source": "/",
      "destination": "https://www.cosaedovemangiare.it",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**/*": {
      "maxDuration": 60
    }
  }
}