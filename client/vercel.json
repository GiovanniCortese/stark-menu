{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "status": 307,
      "headers": { "Location": "https://www.cosaedovemangiare.it" }
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}